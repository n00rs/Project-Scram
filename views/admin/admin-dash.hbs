                                <script>
                                                        window.onload = function () {

                                                            var chart = new CanvasJS.Chart("topSellers", {
                                                                theme: "light2", // "light1", "light2", "dark1"
                                                                animationEnabled: true,
                                                                exportEnabled: true,
                                                                title: {
                                                                    text: "Top 5 Products"
                                                                },
                                                                axisX: {
                                                                    margin: 5,
                                                                    labelPlacement: "inside",
                                                                    tickPlacement: "inside"
                                                                },
                                                                axisY2: {
                                                                    title: "orders",
                                                                    titleFontSize: 14,
                                                                    includeZero: true,
                                                                    
                                                                },
                                                                data: [{
                                                                    type: "bar",
                                                                    axisYType: "secondary",
                                                                    yValueFormatString: "#,###.##nos",
                                                                    indexLabel: "{y}",
                                                                    
                                                                    dataPoints: [
                                                                    {label:'total Orders', y:{{totalSales.totalOrders}} },
                                                                    {{#each  totalSales.topSellers.topSellers}}
                                                                        { label: `{{this.prodName}}`, y: {{this.soldQty}} },
                                                                       
                                                                        {{/each}}
                                                                        
                                                                    ]
                                                                }]
                                                            });
                                                            chart.render();
                                                            var chart1 = new CanvasJS.Chart("6monthsorders", {
	animationEnabled: true,
	title:{
		text: "Orders Per Month"
	},
	axisX:{
		valueFormatString: "MMM"
	},
	axisY: {
		title: "Number of ORDERS",
		scaleBreaks: {
			autoCalculate: true
		}
	},
	data: [{
		type: "line",
		xValueFormatString: "MMM",
		color: "#F08080",
		dataPoints: [

        {{#each totalSales.ordersPerMonth}}
			{ x: new Date(2017,{{this.month}}), y: {{this.orders}} },
            {{/each}}
		]
	}]
});

chart1.render();

                                                        }

                                                    </script>
